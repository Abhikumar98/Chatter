{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [".js", ".jsx"]
			}
		],
		"react/prop-types": 0,
		"no-underscore-dangle": 0,
		"import/newline-after-import": "error",
		"indent": [0, "tab"],
		"prettier/prettier": ["error"]
	}
}
